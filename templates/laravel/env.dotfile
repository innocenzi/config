APP_NAME="@@appname"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=https://@@lowerappname.test

# Database
DB_DATABASE=@@lowerappname
DB_USERNAME=postgres

# Drivers
BROADCAST_DRIVER=log
CACHE_DRIVER=file
