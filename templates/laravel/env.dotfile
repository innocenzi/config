# Mail
MAIL_MAILER=failover
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME="@@appname"
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="no-reply@@@lowerappname.net"
MAIL_FROM_NAME="${APP_NAME}"
